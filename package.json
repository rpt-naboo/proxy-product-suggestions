{
  "name": "proxy-suggestions",
  "version": "1.0.0",
  "description": "Naboo app microservice of suggestions",
  "main": "index.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon --exec node server/app.js",
    "test-server": "NODE_ENV=test jest server/app.test.js --detectOpenHandles --forceExit",
    "db:migration":"cd server && sequelize db:migrate",
    "db:init:seed":"cd server && sequelize db:migrate && sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpt-naboo/proxy-product-suggestions.git"
  },
  "author": "Josh Chen",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "express": "^4.16.4",
    "jest": "^23.6.0",
    "morgan": "^1.9.1",
    "pg": "^7.6.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.41.0",
    "sequelize-cli": "^5.2.0",
    "string-similarity": "^2.0.0",
    "supertest": "^3.3.0"
  }
}
